pipeline{
    agent any 
    stages{
        stage('checkout code'){
            steps{
                checkout scm
            }
        }

        stage('Install Dependencies') {
            steps {
                bat "C:\\Users\\giris\\AppData\\Local\\Programs\\Python\\Python313\\python.exe -m pip install pandas"
            }
        }
        
        stage('Extract Data'){
            steps{
                bat "C:\\Users\\giris\\AppData\\Local\\Programs\\Python\\Python313\\python.exe extract.py"
            }
        }
    }

    post {
        always {
            echo 'pipeline completes.'
        }
        success {
            echo 'pipeline completed'
        }
        failure {
            echo 'pipeline failed'
        }
    }
}